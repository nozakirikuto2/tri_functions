import math
import sys


for line in sys.stdin:
    line = line.strip()
    if not line:
        continue

    try:
        angle_deg = float(line)
        x = angle_deg
    except ValueError:
        continue

    angle_radians = math.radians(x)
    y = angle_radians

    sin = round(math.sin(y), 5)
    cos = round(math.cos(y), 5)
    if cos == 0:
        tan = "無限大"
    else:
        tan = round(math.tan(y), 5)

    print(f"角度: {x}度")
    print(f"sin({x}) = {sin}")
    print(f"cos({x}) = {cos}")
    print(f"tan({x}) = {tan}")
    print()
